package fr.insarouen.asi.prog.asiaventure.elements.vivants;

import fr.insarouen.asi.prog.asiaventure.elements.vivants.VivantException;
import fr.insarouen.asi.prog.asiaventure.elements.vivants.Vivant;

/**
* Classe representant les exceptions dues au fait que l'objet n'est pas possede par le vivant.
*
* @author lavigne_paul
*
*/

public class ObjetNonPossedeParLeVivantException extends VivantException {
  private String msg;

  //constructeurs

/**
* Constructeur ObjetNonPossedeParLeVivantException de la classe de meme nom.
*
*/

  public MondeException(){
	super();
  }

/**
* Constructeur ObjetNonPossedeParLeVivantException de la classe de meme nom.
*
* @param msg
*	Le message envoye dans l'exception.
*
*/

  public ObjetNonPossedeParLeVivantException(String msg){
	super(msg);
  }
}
